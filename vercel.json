{
    "version": 1,
    "builds": [
      {
        "src": "./server.js", 
        "use": "@vercel/node"
      },
      {
        "src": "/client/build",  
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "source": "/(.*)",
        "destination": "./server.js"
      }
    ]
  }